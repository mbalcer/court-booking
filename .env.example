# Environment Variables for Court Booking Application
# Copy this file to .env and customize as needed

# Kafka Configuration
KAFKA_BOOTSTRAP_SERVERS=localhost:9092
KAFKA_TOPIC_BOOKING_CREATED=booking-created

# Database Configuration (when not using H2)
# DB_URL=jdbc:postgresql://localhost:5432/courtbooking
# DB_USERNAME=dbuser
# DB_PASSWORD=dbpass

# Server Configuration
SERVER_PORT=8080

# Logging Level
LOGGING_LEVEL_APP=DEBUG
LOGGING_LEVEL_SQL=DEBUG

# Kafka UI Port (if using docker-compose)
KAFKA_UI_PORT=8090

# Application Profile
SPRING_PROFILES_ACTIVE=default
